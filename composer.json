{
    "name": "arkhe/main",
    "description": "Main part of Arkh√® : the base with User, Permission & Role management",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Arkhe\\Main\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arkhe\\Main\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Luc",
            "email": "luc@adhocrat.io"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/prompts": "*",
        "log1x/laravel-webfonts": "^2.0",
        "ralphjsmit/laravel-seo": "^1.7",
        "spatie/laravel-login-link": "^1.6",
        "spatie/laravel-permission": "^6.21",
        "whitecube/laravel-cookie-consent": "^1.3",
        "livewire/livewire": "^3.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.18",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arkhe\\Main\\ArkheMainServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
